			"browser"
		],
		"rules": {
			"max-params": "off",
			"@typescript-eslint/naming-convention": "off"
		}
	},
	"dependencies": {
		"typed-query-selector": "^2.11.2"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^5.0.0",
		"@types/jsdom": "^21.1.6",
		"jsdom": "^24.0.0",
		"typescript": "^5.4.2",
		"vitest": "^1.3.1",
		"xo": "^0.58.0"
	}
}
