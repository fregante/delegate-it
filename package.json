{
	"name": "delegate-it",
	"version": "6.1.0",
	"description": "Lightweight and modern event delegation in the browser",
	"keywords": [
		"delegate",
		"browser",
		"dom",
		"live",
		"selector",
		"delegation",
		"chrome",
		"electron",
		"firefox",
		"safari",
		"event"
	],
	"repository": "fregante/delegate-it",
	"funding": "https://github.com/sponsors/fregante",
	"license": "MIT",
	"author": "Federico Brigante <me@fregante.com> (https://fregante.com)",
	"type": "module",
	"exports": "./index.js",
	"main": "./index.js",
	"types": "./index.d.ts",
	"files": [
		"index.js",
		"index.d.ts",
		"delegate.js",
		"delegate.d.ts",
		"one-event.js",
		"one-event.d.ts"
	],
	"scripts": {
		"build": "tsc",
		"prepack": "tsc --sourceMap false",
		"test": "tsc && xo && vitest run",
		"watch": "tsc --watch",
		"watch:test": "vitest"
	},
	"xo": {
